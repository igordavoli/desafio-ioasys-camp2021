module.exports = (sequelize, DataTypes) => {
  const Grade = sequelize.define(
    "Grade",
    {

      userId: {
        type: DataTypes.INTEGER,
        field: "user_id",
      },
      movieId: {
        type: DataTypes.INTEGER,
        field: "user_id",
      },
      createdAt: {
        type: DataTypes.DATE,
        field: "created_at",
      },
      updatedAt: {
        type: DataTypes.DATE,
        field: "updated_at",
      },

    },
    { tableName: "movies", }
  );

  return Grade;
};
